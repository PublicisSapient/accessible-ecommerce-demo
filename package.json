{
	"name": "accessible-ecommerce-demo",
	"version": "0.1.0",
	"description": "The Accessible eStore",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PublicisSapient/accessible-ecommerce-demo.git"
	},
	"scripts": {
		"test": "start-server-and-test start-server http://127.0.0.1:8080 pa11y",
		"start": "parcel src/pages/index.html src/pages/**/*.html --no-cache",
		"build": "parcel build src/pages/index.html src/pages/**/*.html --no-cache --public-url /accessible-ecommerce-demo/",
		"deploy": "gh-pages -d dist",
		"pa11y": "pa11y-ci",
		"start-server": "http-server ./dist"
	},
	"devDependencies": {
		"gh-pages": "^2.0.1",
		"handlebars": "^4.1.1",
		"http-server": "^0.11.1",
		"pa11y-ci": "^2.2.0",
		"parcel": "1.12.2",
		"parcel-plugin-asset-copier": "^1.0.0",
		"parcel-plugin-handlebars-precompile": "aevanson/parcel-plugin-handlebars-precompile",
		"sass": "^1.19.0",
		"start-server-and-test": "^1.9.0"
	},
	"dependencies": {
		"pouchdb": "^7.0.0",
		"pouchdb-debug": "^7.0.0",
		"pouchdb-find": "^7.0.0"
	},
	"assetsPath": "src/assets",
	"bugs": {
		"url": "https://github.com/SapientRazorfish/accessible-ecommerce-demo/issues"
	},
	"homepage": "https://github.com/SapientRazorfish/accessible-ecommerce-demo#readme",
	"main": "index.js",
	"author": "",
	"license": "(MIT OR Apache-2.0)"
}
